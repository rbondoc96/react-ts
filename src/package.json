{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch . -e ts,tsx,css,html --ignore build/* --exec \"webpack --config webpack.dev.js\"",
    "dev-server": "webpack serve --config webpack.dev.js",
    "dev": "nodemon --watch . --ignore build/* -e ts,tsx,js,jsx,css,scss,html --exec \"npm run dev-server\"",
    "build": "webpack --config webpack.prod.js",
    "build-dev": "nodemon --watch . -e ts,tsx,scss,html --ignore build/* --exec npm start"
  },
